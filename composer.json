{
  "name": "symfony-notes/json-request-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle for json request ",
  "keywords": ["json", "symfony", "bundle", "rest", "request"],
  "homepage": "https://github.com/symfony-notes/json-request-bundle.git",
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Zhuravel",
      "email": "zhooravell@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SymfonyNotes\\JsonRequestBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require": {
    "php": ">=7.0.15",
    "symfony/framework-bundle": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "phpunit/php-code-coverage": "^5.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
